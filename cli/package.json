{
  "name": "wutenv",
  "description": "Copy and manage app environment variables within development teams",
  "version": "0.0.0",
  "author": "Bryan Donovan @bpdo",
  "bin": {
    "wutenv": "./bin/run"
  },
  "bugs": "https://github.com/bpdo/wutenv/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "axios": "^0.19.0",
    "fs-extra": "^8.0.1",
    "validator": "^11.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "globby": "^8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/bpdo/wutenv",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "bin": "wutenv",
    "commands": "./src/commands",
    "hooks": {
      "init": "./src/hooks/init/config"
    },
    "topics": {
      "env": {
        "description": "manage environment variables"
      }
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "bpdo/wutenv",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
